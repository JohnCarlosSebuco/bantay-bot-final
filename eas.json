{
  "cli": {
    "version": ">= 12.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "channel": "preview"
    },
    "preview-gradle": {
      "android": {
        "gradleCommand": ":app:assembleRelease"
      },
      "channel": "preview-gradle"
    },
    "preview-internal": {
      "distribution": "internal",
      "channel": "preview-internal"
    },
    "production": {
      "android": {
        "buildType": "app-bundle"
      },
      "channel": "production"
    }
  }
}
